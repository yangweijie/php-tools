{
    "name": "yangweijie/php-tools",
    "description": "tookit gui wrote by PHP",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/yangweijie/php-tools/issues",
        "source": "https://github.com/yangweijie/php-tools"
    },
    "authors": [
        {
            "name": "yangweijie",
            "email": "917647288@qq.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "./kingbes/libui",
            "options": {
                "symlink": false
            }
        }
    ],
    "require": {
        "php": "^8.2",
        "kingbes/libui": "0.0.1",
        "laravel-zero/framework": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.22",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.8.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "kingbes/libui": "source",
            "*": "dist"
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["toolkit"]
}
