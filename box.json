{
    "chmod": "0755",
    "directories": [
        "app",
        "vendor/kingbes/libui/src",
        "vendor/kingbes/libui/lib"
    ],
    "files": [
        "cli.php",
        "composer.json",
        "vendor/autoload.php",
        "vendor/composer/autoload_classmap.php",
        "vendor/composer/autoload_files.php",
        "vendor/composer/autoload_namespaces.php",
        "vendor/composer/autoload_psr4.php",
        "vendor/composer/autoload_real.php",
        "vendor/composer/autoload_static.php",
        "vendor/composer/ClassLoader.php",
        "vendor/kingbes/libui/composer.json",
        "vendor/kingbes/libui/LICENSE",
        "vendor/kingbes/libui/README.md"
    ],
    "exclude-composer-files": true,
    "compression": "GZ",
    "compactors": [
        "KevinGH\\Box\\Compactor\\Php",
        "KevinGH\\Box\\Compactor\\Json"
    ],
    "main": "cli.php",
    "output": "builds/tools.phar",
    "alias": "tools.phar",
    "banner": "PHP Tools PHAR",
    "stub": true
}